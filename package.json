{
  "name": "rcc-calendar",
  "version": "0.0.1",
  "description": "a react Calendar of the React Component Collection.",
  "main": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "source": "src/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/umd/index.js"
    },
    "./dist/Calendar.css": "./dist/Calendar.css"
  },
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "scripts": {
    "build": "yarn build-js-all && yarn copy-styles && yarn build-styles",
    "build-js-all": "yarn build-js-esm && yarn build-js-umd",
    "build-js-esm": "BABEL_ENV=production-esm babel src -d dist/esm --ignore \"**/*.spec.js,**/*.spec.jsx\"",
    "build-js-umd": "BABEL_ENV=production-umd babel src -d dist/umd --ignore \"**/*.spec.js,**/*.spec.jsx\"",
    "build-styles": "lessc ./dist/Calendar.less ./dist/Calendar.css",
    "clean": "rimraf dist",
    "copy-styles": "node ./copy-styles.js",
    "jest": "jest",
    "lint": "eslint sample/ src/ test/ --ext .jsx,.js",
    "prepack": "yarn clean && yarn build",
    "test": "yarn lint && yarn jest"
  },
  "keywords": [
    "calendar",
    "date",
    "date-picker",
    "month-picker",
    "react"
  ],
  "author": {
    "name": "Wojciech Maj",
    "email": "kontakt@wojtekmaj.pl"
  },
  "contributors": [
    {
      "name": "Alberto Carreras"
    },
    {
      "name": "Andriy Kuzyk"
    },
    {
      "name": "Baldur √Årnason"
    },
    {
      "name": "Jack Willis-Craig"
    },
    {
      "name": "JamieEgeberg"
    },
    {
      "name": "Justin Davis"
    },
    {
      "name": "Kevin Kipp",
      "email": "kevin.kipp@gmail.com"
    },
    {
      "name": "Ritesh Kumar",
      "email": "rkritesh078@gmail.com"
    },
    {
      "name": "Sergi Meseguer"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@wojtekmaj/date-utils": "^1.0.2",
    "get-user-locale": "^1.2.0",
    "merge-class-names": "^1.1.1",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.0",
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "enzyme": "^3.10.0",
    "eslint": "~7.19.0",
    "eslint-config-wojtekmaj": "^0.5.0",
    "jest": "^27.0.0",
    "less": "^4.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rimraf": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^16.3.0 || ^17.0.0-0",
    "react-dom": "^16.3.0 || ^17.0.0-0"
  },
  "resolutions": {
    "semver@7.0.0": "^7.0.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist/",
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/react-component-collection/calendar.git"
  }
}
